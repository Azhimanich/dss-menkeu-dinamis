{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/home",
      "dest": "/home.html"
    },
    {
      "src": "/how-it-works",
      "dest": "/how-it-works.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}

